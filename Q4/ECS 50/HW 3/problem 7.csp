A:	.WORD 23
COUNT:	.WORD 0
EVEN:	.WORD 0
PARITY:	.WORD $FFF
RESULT:	.WORD $000
	JSR $E01
	STA PARITY
LOOP:	SHLA
	JOV INCREMENT
RETURN: 	INC COUNT
	STA RESULT
	LDA A
	CMA COUNT
	LDA RESULT
	JLT CALCULATE
	JMP LOOP
INCREMENT: 	INC EVEN
	JMP RETURN
CALCULATE: 	LDA EVEN
	MOD# $002
	JEQ END
	LDA PARITY
	ADA# $800
	STA PARITY
END: 	LDA PARITY
	JSR $E00
	HLT

