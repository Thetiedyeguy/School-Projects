CRITICAL: 	.WORD $000
N: 	.WORD $000
COUNT: 	.WORD $000
GREATER: 	.WORD $000
	JSR $E01
	STA CRITICAL
STOREAMOUNT:JSR $E01
	CMA# 0
	JLT REDO
	CMA# 0
	JLT REDO
	STA N
	JMP LOOP
REDO:	LDA# $000
	JSR $E00
	JMP STOREAMOUNT
LOOP: 	JSR $E01
	CMA CRITICAL
	JLT LESSTHAN
	INC GREATER
LESSTHAN: 	INC COUNT
	LDA COUNT
	CMA N
	JLT LOOP
	LDA N
	SBA GREATER
	JSR $E00
	LDA GREATER
	JSR $E00
	HLT